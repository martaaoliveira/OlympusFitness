{
  "expo": {
    "name": "Olympo",
    "slug": "Olympo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.OlympoFintess.web"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.OlympoFintess.web"
    },
    "web": {
      "build": {
        "outputDir": "web-build"
      },
      "publicPath": "./",
      "entryPoint": "src/index.js"
    }
  }
}
